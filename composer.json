{
	"name": "nextgenthemes/advanced-responsive-video-embedder",
	"description": "ARVE Advanced Responsive Video Embedder",
	"type": "wordpress-plugin",
	"require": {
		"php": ">=7.4"
	},
	"require-dev": {
		"nextgenthemes/wp-shared": "@dev",
		"automattic/jetpack-autoloader": "^3.0.0"
	},
	"license": "GPL-3.0",
	"authors": [
		{
			"name": "Nicolas Jonas"
		}
	],
	"config": {
		"allow-plugins": {
			"automattic/jetpack-autoloader": true
		},
		"optimize-autoloader": true,
		"platform": {
			"php": "7.4"
		}
	},
	"repositories": {
		"nextgenthemes/wp-shared": {
			"type": "path",
			"url": "../nextgenthemes/wp-shared",
			"options": {
				"symlink": false
			}
		}
	},
	"autoload": {
		"psr-4": {
			"ARVE\\": "includes/"
		},
		"files": [
			"includes/ARVE/fn-deprecated.php",
			"includes/ARVE/fn-compat.php",
			"includes/ARVE/fn-assets.php",
			"includes/ARVE/fn-html-output.php",
			"includes/ARVE/fn-misc.php",
			"includes/ARVE/fn-oembed.php",
			"includes/ARVE/fn-shortcode-data.php",
			"includes/ARVE/fn-shortcode-args.php",
			"includes/ARVE/fn-shortcodes.php",
			"includes/ARVE/fn-url-handlers.php",
			"includes/ARVE/fn-validation.php",
			"includes/ARVE/fn-settings.php",
			"includes/ARVEAdmin/load-admin-files.php"
		]
	}
}
