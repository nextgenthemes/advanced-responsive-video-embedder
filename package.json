  "scripts": {
    "start:symbiosis": "wp-scripts start themes/symbiosis/src/ts/main themes/symbiosis/src/ts/essential --output-path=themes/symbiosis/build",
    "build:symbiosis": "wp-scripts build themes/symbiosis/src/ts/main themes/symbiosis/src/ts/essential --output-path=themes/symbiosis/build",
    "start:blocks": "wp-scripts start --webpack-src-dir=plugins/nextgenthemes-blocks/src --output-path=plugins/nextgenthemes-blocks/build --webpack-copy-php",
    "build:blocks": "wp-scripts build --webpack-src-dir=plugins/nextgenthemes-blocks/src --output-path=plugins/nextgenthemes-blocks/build --webpack-copy-php",
    "start:advanced-responsive-video-embedder": "wp-scripts start --output-path=plugins/advanced-responsive-video-embedder/build",
    "build:advanced-responsive-video-embedder": "wp-scripts build --output-path=plugins/advanced-responsive-video-embedder/build",
    "start:arve-pro": "wp-scripts start plugins/arve-pro/src/main --output-path=plugins/arve-pro/build",
    "build:arve-pro": "wp-scripts build plugins/arve-pro/src/main --output-path=plugins/arve-pro/build",
    "start:arve-random-video": "wp-scripts start",
    "build:arve-random-video": "wp-scripts build",
    "start:arve-sticky-videos": "wp-scripts start",
    "build:arve-sticky-videos": "wp-scripts build",
    "start:edd-bootstrap-styles": "wp-scripts start ../../../nextgenthemes.com/web/app/plugins/edd-bootstrap-styles/src/main --output-path=../../../nextgenthemes.com/web/app/plugins/edd-bootstrap-styles/build",
    "build:edd-bootstrap-styles": "wp-scripts build ../../../nextgenthemes.com/web/app/plugins/edd-bootstrap-styles/src/main --output-path=../../../nextgenthemes.com/web/app/plugins/edd-bootstrap-styles/build",
    "build": "npm-run-all build:*",
    "check-engines": "wp-scripts check-engines",
    "check-licenses": "wp-scripts check-licenses",
    "format:js": "wp-scripts format-js",
    "lint:css": "wp-scripts lint-style",
    "lint:js": "wp-scripts lint-js \"plugins/advanced-responsive-video-embedder/src/**\" \"plugins/arve-*/src/**\" \"themes/symbiosis/src/ts/**\"",
    "lint:md:docs": "wp-scripts lint-md-docs",
    "lint:md:js": "wp-scripts lint-md-js",
    "lint:pkg-json": "wp-scripts lint-pkg-json",
    "packages-update": "wp-scripts packages-update",
    "test:e2e": "wp-scripts test-e2e",
    "test:unit": "wp-scripts test-unit-js",
    "SEPP": "SEP",
    "update:composer": "composer update",
    "update:npm": "npm update; npm audit fix; npm audit || true",
    "update": "npm run update:composer & npm run update:npm; composer outdated & npm outdated",
    "wp-env": "wp-env",
    "wp-env:restart": "wp-env stop; wp-env start",
    "wp-env-s": "wp-env run cli \"wp plugin delete akismet hello.php\"",
    "browsersync": "browser-sync start --proxy symbiosistheme.test --files \"**/build/*\" \"**/*.php\"",
    "copy-common": "cp plugins/advanced-responsive-video-embedder/php/Common/functions-assets.php themes/symbiosis/php/Common2/ && cp plugins/advanced-responsive-video-embedder/php/Common/functions-async-defer.php themes/symbiosis/php/Common2/"
